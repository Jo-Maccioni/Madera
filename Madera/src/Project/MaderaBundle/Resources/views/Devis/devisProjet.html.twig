<html>
	<head>
		<meta charset="utf-8">
		<title>MADERA</title>
		
		<!-- CSS boostrap -->
		<link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
		<!-- theme boostrap -->
		<link rel="stylesheet" href="bootstrap/css/themes/flatly.min.css">

		<!-- JS boostrap -->
		<script src="bootstrap/js/jquery.js"></script>
		<script src="bootstrap/js/bootstrap.min.js"></script>
	</head>
	
	<body>
		<nav class="navbar navbar-default">
			<div class="container-fluid">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="index">MADERA</a>
				</div>

				<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
					<ul class="nav navbar-nav">
						<li><a href="#">test</a></li>
					</ul>
				</div>
			</div>
		</nav>
		
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<div style="float: left">
						<h3 style="margin-top: 0px">Création d'un nouveau devis</h3>
					</div>
				</div>
			</div>
			
			<div class="row">
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4"></div>
				<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
					<form class="form-horizontal" method="post" action="create-devis">
						<div class="form-group">
							<label for="selectClient" class="col-sm-6 control-label">Selection du client</label>
							<div class="col-sm-6">
								<select class="form-control" name="selectClient">
									<option value="0" selected>Liste Client</option>
									<option value="refClient1">Client 1</option>
									<option value="refClient2">Client 2</option>
									<option value="refClient3">Client 3</option>
									<option value="refClient4">Client 4</option>
								</select>
							</div>
						</div>
						<div class="form-group">
							<label for="selectProjet" class="col-sm-6 control-label">Selection du projet</label>
							<div class="col-sm-6">
								<select class="form-control" name="selectProjet">
									<option value="0" selected>Liste Projet</option>
									<option value="projet1">Projet 1</option>
									<option value="projet2">Projet 2</option>
									<option value="projet3">Projet 3</option>
									<option value="projet4">Projet 4</option>
								</select>
							</div>
						</div>
						
						<button class="btn btn-success btn-sm" style="width: 100%">Passer à l'étape suivante</button>
					</form>
				</div>
			</div>
		</div>
	</body>
</html>